<p id="notice"><%= notice %></p>

<h1>Mvas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mvas.each do |mva| %>
      <tr>
        <td><%= link_to 'Show', mva %></td>
        <td><%= link_to 'Edit', edit_mva_path(mva) %></td>
        <td><%= link_to 'Destroy', mva, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mva', new_mva_path %>
